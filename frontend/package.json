  "scripts": {
    "refresh": "npm install && npm prune",
    "reset": "rm -rf node_modules && npm run refresh && ./node_modules/.bin/webpack",
    "build-dev": "rm -rf dist && webpack --config config/webpack.dev.js",
    "build-prod": "rm -rf dist && webpack --config config/webpack.prod.js",
    "start": "webpack -w",
    "pretest": "rm -rf dist && webpack --config config/webpack.test.js",
    "test": "npm run phantom-test",
    "phantom-test": "node --max_old_space_size=4096 ./node_modules/karma/bin/karma start --browsers PhantomJS karma.conf.js",
    "firefox-test": "karma start --browsers Firefox karma.conf.js",
    "chrome-test": "karma start --browsers Chrome karma.conf.js",
    "sauce-test": "karma start config/saucelabs.karma.conf.js",
    "debug": "karma start --browsers Chrome karma.conf.js --no-single-run",
    "tslint": "tslint `ls src/**/*.ts | grep -v .d.ts$` spec/**/*.ts",
    "tslint-src": "tslint -p src/tsconfig.json"
  },
